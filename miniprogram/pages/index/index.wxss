/**index.wxss**/
page {
    height: 100%;
    width: 100%;
    background: #f6f6f6;
    display: flex;
    flex-direction: column;
    justify-content: space-between;

}

.container {
    height: 100%;
    width: 100%;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    align-items: center;
    background: #b4af9f;
}

.usertags {
    margin: auto;
}

.no-more-data {
    margin: auto;
    font-size: 24rpx;
}

.tag {
    display: inline-block;
    font-size: 28rpx;
    position: relative;
}

.tag:hover {
    border: 0.1rpx dashed #cfcfcf;
}
